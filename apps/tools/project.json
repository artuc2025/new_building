{
  "name": "tools",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tools/src",
  "projectType": "application",
  "targets": {
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/seed/index.ts",
        "cwd": "{projectRoot}"
      }
    },
    "db:spatial-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/scripts/spatial-check.ts",
        "cwd": "{projectRoot}"
      }
    },
    "db:perf": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/scripts/perf-test.ts",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": []
}

